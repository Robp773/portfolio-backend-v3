{
  "kind": "singleType",
  "collectionName": "globals",
  "info": {
    "name": "Website Data",
    "description": "Website meta data"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "favicon": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "siteName": {
      "type": "string"
    },
    "defaultSeo": {
      "type": "component",
      "repeatable": false,
      "component": "global.default-seo"
    },
    "placeHolder": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    }
  }
}
